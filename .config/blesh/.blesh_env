ble-bind -c 'C-e (' '__exe python3 ~/.config/subset/subset.py -a -d dev_env -i 8'
ble-bind -c 'C-e )' '__exe python3 ~/.config/subset/subset.py -a -d dev_env -i 9'

ble-bind -c 'C-e 0' '__exe python3 ~/.config/subset/subset.py -s -d dev_env -i 0'
