ble-bind -c 'C-e x' 'exit'
ble-bind -c 'C-e +' 'amixer -D pulse sset Master 5%+'
ble-bind -c 'C-e -' 'amixer -D pulse sset Master 5%-'
ble-bind -c 'C-e l' 'python3 ~/.config/subset/subset.py -d dev_env -l'

ble-bind -c 'C-e =' '__exe python3 ~/.config/subset/subset.py -a -d dev_env -i 0'
ble-bind -c 'C-e !' '__exe python3 ~/.config/subset/subset.py -a -d dev_env -i 1'
