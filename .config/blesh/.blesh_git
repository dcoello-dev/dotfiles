source $HOME/.config/blesh/core.sh

ble-bind -c 'C-g h' '__exe git log --oneline -n10'
ble-bind -c 'C-g s' '__exe git status'
ble-bind -c 'C-g a' '__exe git add --all'
ble-bind -c 'C-g f' '__exe git fetch'
ble-bind -c 'C-g p' '__exe git push -f'
ble-bind -c 'C-g g' '__exe git pull'
ble-bind -c 'C-g c' '__exe git commit -s'
ble-bind -c 'C-g m' '__exe git commit -s --amend'
ble-bind -c 'C-g <' '__exe git rebase --continue'

ble-bind -c 'C-g l' '__exe python3 ~/.config/.subset/subset.py -d git -l'
ble-bind -c 'C-g u' '__exe python3 ~/.config/.subset/subset.py -a -d git -i 0'
ble-bind -c 'C-g i' '__exe python3 ~/.config/.subset/subset.py -a -d git -i 1'
ble-bind -c 'C-g o' '__exe python3 ~/.config/.subset/subset.py -a -d git -i 2'
ble-bind -c 'C-g U' '__exe python3 ~/.config/.subset/subset.py -s -d git -i 0'
ble-bind -c 'C-g I' '__exe python3 ~/.config/.subset/subset.py -s -d git -i 1'
ble-bind -c 'C-g O' '__exe python3 ~/.config/.subset/subset.py -s -d git -i 2'
ble-bind -c 'C-g R' '__exe python3 ~/.config/.subset/subset.py -d git --reset'
